{
  "name": "express-notes-api",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start-ts": "ts-node-esm server.ts",
    "start": "node server.js",
    "dev-ts": "nodemon server.ts",
    "dev": "nodemon server.js"
  },
  "nodemonConfig": {
    "ignore": [
      "data.json"
    ],
    "execMap": {
      "ts": "ts-node-esm"
    }
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.1",
    "nodemon": "^2.0.2"
  }
}
